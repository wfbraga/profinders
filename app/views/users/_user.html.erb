      <tr id="user_row<%= user.id %>">
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.city %></td>
        <td><%= user.state %></td>
        <td><%= user.country %></td>
        <td><%= user.contact_number %></td>
        <td><%= link_to 'Show', user_path(user), class: 'btn btn-info'%></td>
        <td><%= link_to 'Edit', edit_user_path(user), class: 'btn btn-warning' %></td>
        <td><%= link_to 'Destroy', user_path(user), method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></td>
      </tr>
